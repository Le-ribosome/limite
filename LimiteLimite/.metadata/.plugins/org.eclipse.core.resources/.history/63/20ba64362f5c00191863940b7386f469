package fr.formation.cartes;
import java.util.ArrayList;
import java.util.List;

import javax.persistence.MappedSuperclass;


@MappedSuperclass
public class Carte {
	
	protected int id;
	protected List<Carte> listeCartes = new ArrayList<Carte>();
	protected String categorie;
	
	public Carte() {
		this.id = gestionid++;
		listeCartes.add(this);
	}
	


	public List<Carte> getListeCartes() {
		return listeCartes;
	}



	public void setListeCartes(List<Carte> listeCartes) {
		this.listeCartes = listeCartes;
	}



	public String getSabot() {
		return sabot;
	}



	public void setSabot(String sabot) {
		this.sabot = sabot;
	}



	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	
	
}


