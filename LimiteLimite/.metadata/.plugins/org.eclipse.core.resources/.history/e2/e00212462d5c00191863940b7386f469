package fr.formation.cartes;
import java.util.ArrayList;

@Entity
@Table()
public class Carte {
	
	private String sabot;
	private static int gestionid = 1;
	private int id;
	private List<Carte> listeCartes = new ArrayList<Carte>();
	
	
	public Carte() {
		this.id = gestionid++;
		listeCartes.add(this);
	}
	


	public ArrayList<Carte> getListeCartes() {
		return listeCartes;
	}



	public void setListeCartes(ArrayList<Carte> listeCartes) {
		this.listeCartes = listeCartes;
	}



	public String getSabot() {
		return sabot;
	}



	public void setSabot(String sabot) {
		this.sabot = sabot;
	}



	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	
	
}


